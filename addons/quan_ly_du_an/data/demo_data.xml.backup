            <field name="doanh_thu_thuc_te">0</field>
            <field name="do_uu_tien">3_cao</field>
            <field name="trang_thai">hoan_thanh</field>
            <field name="tien_do">100.0</field>
            <field name="muc_do_rui_ro">thap</field>
            <field name="thanh_vien_ids" eval="[(6, 0, [ref('nhan_su.demo_nhan_vien_1'), ref('nhan_su.demo_nhan_vien_2'), ref('nhan_su.demo_nhan_vien_3'), ref('nhan_su.demo_nhan_vien_4')])]"/>
